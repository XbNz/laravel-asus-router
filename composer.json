{
    "name": "xbnz/laravel-asus-router",
    "description": "Laravel package for accessing your ASUS Merlin router's statistics",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "AG",
            "email": "fdwnct@gmail.com"
        }
    ],
    "require": {
        "orchestra/testbench": "^6.0",
        "spatie/ssh": "^1.6",
        "spatie/data-transfer-object": "^3.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "XbNz\\AsusRouter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XbNz\\AsusRouter\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "XbNz\\AsusRouter\\AsusRouterServiceProvider"
            ]
        }
    }
}
